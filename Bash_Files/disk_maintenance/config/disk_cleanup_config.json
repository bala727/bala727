  ],
  "file_delete_list": [
    {
      "path": "/jidoka/plc_ftp_logs/",
      "delete_older_than_days": 3
    },
    {
      "path": "/jidoka/v*/logs/",
      "delete_older_than_days": 30
    },
    {
      "path": "/jidoka/v*/images/filewatch/",
      "delete_older_than_days": 7
    },
    {
      "path": "/jidoka/cctv/recordings/",
      "delete_older_than_days": 3
    }
  ]
}